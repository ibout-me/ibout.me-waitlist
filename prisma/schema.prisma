generator client {
  provider = "prisma-client"
  output   = "./generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model WaitlistEntry {
  id         Int       @id @default(autoincrement())
  email      String    @unique
  createdAt  DateTime  @default(now()) @map("created_at")
  notifiedAt DateTime? @map("notified_at")

  @@map("waitlist_entries")
}
